{
  "name": "@ten/locale",
  "version": "0.0.1",
  "main": "src/index.js",
  "description": "Framework to allow for locale differences in Vue",
  "license": "MIT",
  "homepage": "https://github.com/tencodigo/ten-locale",
  "repository": "git@github.com:tencodigo/ten-locale.git",
  "authors": [
    "Chris Doty <ten@tencodigo.org>"
  ],
  "scripts": {
    "semantic-release": "semantic-release"
  },
  "engines": {
    "yarn": ">= 1.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.2",
    "eslint": "^4.12.1",
    "husky": "^1.3.1"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "js-joda": "^1.11.0",
    "js-joda-timezone": "^2.1.1",
    "lodash": "^4.17.15",
    "q": "^1.5.1",
    "vue": "^3.0.0"
  },
  "eslintConfig": {
    "parserOptions": {
      "parser": "babel-eslint",
      "ecmaVersion": 2017,
      "sourceType": "module"
    },
    "extends": "@hydrant/eslint-config/vue",
    "rules": {
      "vue/max-attributes-per-line": "off",
      "vue/no-use-v-if-with-v-for": "off",
      "vue/no-unused-components": "off",
      "vue/component-name-in-template-casing": "off",
      "vue/singleline-html-element-content-newline": "off",
      "vue/no-v-html": "off",
      "vue/require-default-prop": "off",
      "vue/use-v-on-exact": "off",
      "vue/multiline-html-element-content-newline": "off"
    }
  },
  "resolutions": {
    "which": "~1.3.1"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
